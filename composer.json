{
  "name": "kucoder/kucoder",
  "type": "project",
  "keywords": [
    "vue",
    "Element Plus",
    "framework",
    "high performance",
    "http service"
  ],
  "homepage": "https://kucoder.com",
  "license": "Apache-2.0",
  "description": "kucoder-开源免费高性能PHP框架 基于Workerman + Webman + PHP8 + Vue3 + ElementPlus + JavaScript(非ts)",
  "authors": [
    {
      "name": "walkor",
      "email": "walkor@workerman.net",
      "homepage": "https://www.workerman.net",
      "role": "Developer"
    },
    {
      "name": "kucoder",
      "email": "kucoder@qq.com",
      "homepage": "https://kucoder.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "kucoder@qq.com",
    "forum": "https://kucoder.com/question",
    "wiki": "https://doc.kucoder.com",
    "source": "https://github.com/himfkc/kucoder"
  },
  "require": {
    "php": ">=8.1",
    "workerman/webman-framework": "^2.1",
    "monolog/monolog": "^2.0",
    "webman/console": "^2.1",
    "webman/think-orm": "^2.1",
    "webman/think-cache": "^2.1",
    "webman/redis": "^2.1",
    "illuminate/events": "^12.14",
    "firebase/php-jwt": "^6.11",
    "gregwar/captcha": "^1.2",
    "webman/log": "~2.1",
    "webman/event": "^1.0",
    "symfony/process": "^7.3",
    "symfony/var-dumper": "^7.3",
    "guzzlehttp/guzzle": "^7.9",
    "webman/rate-limiter": "^1.1",
    "vlucas/phpdotenv": "^5.6",
    "ramsey/uuid": "^4.9",
    "foroco/php-browser-detection": "^2.8",
    "php-di/php-di": "^7.0",
    "workerman/http-client": "^3.0",
    "ext-bcmath": "*",
    "ext-redis": "*",
    "ext-fileinfo": "*",
    "ext-openssl": "*",
    "ext-sodium": "*",
    "ext-zip": "*",
    "ext-pdo": "*",
    "ext-curl": "*"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app",
      "App\\": "./app",
      "app\\View\\Components\\": "./app/view/components",
      "kucoder\\": "./plugin/kucoder/app/kucoder",
      "Kucoder\\": "./plugin/kucoder/app/kucoder"
    }
  },
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::update"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
